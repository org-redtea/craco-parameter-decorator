{
  "name": "@redtea/craco-parameter-decorator",
  "version": "1.0.4",
  "description": "This craco plugin will set up babel-plugin-parameter-decorator for your project.",
  "type": "commonjs",
  "types": "/index.d.ts",
  "main": "./lib/index.js",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./mjs-wrappers/index.mjs"
    },
    "./babel-preset-react-app-override": {
      "require": "./lib/babel-preset-react-app-override.js",
      "import": "./mjs-wrappers/babel-preset-react-app-override.mjs"
    }
  },
  "sideEffects": false,
  "keywords": [
    "craco",
    "react",
    "react-app",
    "typescrit",
    "babel",
    "babel-plugin-parameter-decorator"
  ],
  "files": [
    "lib/*.js",
    "mjs-wrappers/*.mjs",
    "index.d.ts",
    "./README.md"
  ],
  "scripts": {
    "preversion": "package-check"
  },
  "author": "Kirill Khoroshilov <mr.hokid@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/org-redtea/craco-parameter-decorator.git"
  },
  "peerDependencies": {
    "@craco/craco": "~5 || ~6",
    "react-scripts": "~3 || ~4"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "shx": "0.3.3"
  },
  "dependencies": {
    "babel-plugin-parameter-decorator": "1.0.16"
  }
}
